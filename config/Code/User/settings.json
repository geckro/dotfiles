{
    "catppuccin.accentColor": "lavender",
    "cwtools.cache.hoi4": "d:\\SteamLibrary\\steamapps\\common\\Hearts of Iron IV",
    "diffEditor.codeLens": true,
    "diffEditor.experimental.showMoves": true,
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 8,
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.cursorWidth": 2,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "'Iosevka Curly Slab', 'JetBrains Mono', 'Cascadia Code', 'Fira Code', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontVariations": true,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.guides.bracketPairs": "active",
    "editor.lineHeight": 24,
    "editor.lineNumbers": "relative",
    "editor.minimap.maxColumn": 80,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fit",
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.stickyScroll.enabled": true,
    "editor.wordSeparators": "`~!@#$%^&a*()-=+[{]}\\|;:'\",.<>/?",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.eol": "\n",
    "files.exclude": {
        "**/.DS_Store": false,
        "**/.hg": false,
        "**/.svn": false,
        "**/CVS": false,
        "**/Thumbs.db": false
    },
    "files.insertFinalNewline": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "always",
    "git.untrackedChanges": "separate",
    "search.exclude": {
        "**/.github": true,
        "**/.git": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.stickyScroll.enabled": true,
    "update.mode": "start",
    "update.showReleaseNotes": false,
    "window.autoDetectColorScheme": true,
    "window.commandCenter": false,
    "window.dialogStyle": "custom",
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "window.zoomLevel": 1,
    "workbench.activityBar.location": "top",
    "workbench.colorTheme": "Catppuccin Latte",
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 6,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.preferredDarkColorTheme": "Catppuccin Frapp√©",
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "workbench.productIconTheme": "material-product-icons",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 12,
    "workbench.view.alwaysShowHeaderActions": true
}
